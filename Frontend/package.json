{
  "name": "nts23",
  "version": "0.0.0-development",
  "private": true,
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "out": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build",
    "format": "prettier --write .",
    "cz-init": "commitizen init cz-conventional-changelog --dev --exact",
    "commit": "cz",
    "update": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "axios": "^1.3.5",
    "gsap": "npm:@gsap/shockingly",
    "intl": "^1.2.5",
    "next": "13.3.0",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.1",
    "npm": "^9.6.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "react-wrap-balancer": "^0.4.0",
    "tss-react": "^4.8.2",
    "type-fest": "^3.8.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@emotion/server": "^11.10.0",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@next/bundle-analyzer": "^13.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/intl": "^1.2.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-memo": "^0.0.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tss-unused-classes": "^0.0.4",
    "file-loader": "^6.2.0",
    "glslify": "^7.1.1",
    "glslify-loader": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "next-strict-csp": "^1.0.1",
    "prettier": "2.8.7",
    "raw-loader": "^4.0.2",
    "react-aria": "^3.24.0",
    "tsafe": "^1.6.0",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1",
    "utility-types": "^3.10.0",
    "webpack-favicons": "^1.3.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableScopeLowerCase": false,
      "disableSubjectLowerCase": false,
      "maxHeaderWidth": 100,
      "maxLineWidth": 100,
      "defaultType": "",
      "defaultScope": "",
      "defaultSubject": "",
      "defaultBody": "",
      "defaultIssues": "",
      "types": {
        "feat": {
          "title": "feat",
          "description": "New feature | MINOR"
        },
        "fix": {
          "title": "fix",
          "description": "A Bug fix | PATCH"
        },
        "docs": {
          "title": "docs",
          "description": "Documentation changes | PATCH"
        },
        "style": {
          "title": "style",
          "description": "Formatting and style changes | PATCH"
        },
        "refactor": {
          "title": "refactor",
          "description": "Improvements that do not affect functionality (RMSE ref commitlint) | PATCH"
        },
        "perf": {
          "title": "perf",
          "description": "A performance improvement | PATCH"
        },
        "test(feat)": {
          "title": "test(feat)",
          "description": "Added new test | MINOR"
        },
        "test(fix)": {
          "title": "test(fix)",
          "description": "Fixing an existing test | PATCH"
        },
        "chore": {
          "title": "chore",
          "description": "Changes to the build process or auxiliary tools and libraries | PATCH"
        },
        "wai-fix": {
          "title": "wai-fix",
          "description": "An accessibility bug fix  | PATCH"
        },
        "wai-feat": {
          "title": "wai-fix",
          "description": "New accessibility feature | MINOR"
        },
        "security-fix": {
          "title": "security-fix",
          "description": "Security bug fix | PATCH"
        },
        "security-feat": {
          "title": "security-feat",
          "description": "New security feature | MINOR"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vladimirdevicofficial/NTS23.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
