{
  "endOfLine": "lf",
  "printWidth": 80,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": true,
  "semi": true,
  "quoteProps": "preserve",
  "jsxSingleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "requirePragma": true,
  "insertPragma": true,
  "proseWrap": "never",
  "singleAttributePerLine": true,
  "importOrderSeparation": false
}
